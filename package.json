{
  "name": "el_visionat_scripts",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "firebase-admin": "^12.0.0"
  },
  "scripts": {
    "seed": "node --loader ts-node/esm --no-warnings scripts/seed_registry.ts",
    "seed:voting": "node --loader ts-node/esm --no-warnings scripts/seed_voting.ts",
    "seed:emu": "cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8088 npm run seed && cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8088 npm run seed:voting"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
